     6        SUBROUTINE VEC (N,A,MAXA,AA)
     7  C      
     8  C     MATRIXCOPY receives the matrix A and outputs
     9  C         the matrix B = A
    10  C
    11        IMPLICIT REAL*8 (A-H,O-Z)
    12        DIMENSION A(MAXA,MAXA),AA(66000)
    13  C
    14  C     A=input matrix
    15  C     AA=output vector
    16  C     MAXA=maximum dimension of A (MUST BE SQUARE)
    17  C     N=square dimension of matrix (i.e. 3 if A->3x3 matrix)
    18  C
    19            II=0
    20                DO 1000 I=1,N
    21                DO 1000 J=1,N
    22                    II=II+1
    23                    AA(II)=A(I,J)
    24   1000         CONTINUE 
    25  C
    26        RETURN
    27        END

